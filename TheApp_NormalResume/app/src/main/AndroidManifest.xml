<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="thenewboston.com.theapp_normalresume">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Blank" />
        <activity
            android:name=".MyActivity"
            android:label="@string/title_activity_my"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".WebCal" />
        <activity android:name=".WebMar" />
        <activity android:name=".Web_Sav" />
        <activity android:name=".WebLis" />
        <activity android:name=".WebAt"></activity>
    </application>

</manifest>